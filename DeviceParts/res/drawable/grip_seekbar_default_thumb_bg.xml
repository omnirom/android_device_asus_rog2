<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/item_bg">
        <shape android:shape="oval">
            <solid android:color="@color/settings_default_color"/>
            <size android:height="24dp" android:width="24dp"/>
        </shape>
    </item>
    <item android:left="6dp" android:top="6dp" android:right="6dp" android:bottom="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/settings_default_color"/>
            <size android:height="18dp" android:width="18dp"/>
        </shape>
    </item>
    <item android:left="8dp" android:top="8dp" android:right="8dp" android:bottom="8dp">
        <shape android:shape="oval">
            <solid android:color="@color/default_background_color"/>
            <size android:height="16dp" android:width="16dp"/>
        </shape>
    </item>
</layer-list>
