/asdf(/.*)?             u:object_r:asdf_file:s0
/batinfo(/.*)?          u:object_r:demoapp_file:s0
/voucher(/.*)?          u:object_r:voucher_file:s0
/xrom(/.*)?             u:object_r:xrom_file:s0

# Audio
/system/etc/audio_policy_configuration.xml    u:object_r:system_configs_file:s0

